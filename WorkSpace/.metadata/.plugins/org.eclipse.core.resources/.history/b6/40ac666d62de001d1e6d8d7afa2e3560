package pack04_oop2;

public class StudentDTO {
	int num;
	String name;
	int korSco;
	int matSco;
	int engSco;
	int sum;
	double avg;
	public StudentDTO(int num, String name, int korSco, int matSco, int engSco) {
		
		this.num = num;
		this.name = name;
		this.korSco = korSco;
		this.matSco = matSco;
		this.engSco = engSco;
		this.sum=this.korSco+this.matSco+this.engSco;
		this.avg=(double)(this.korSco+this.matSco+this.engSco)/3;
	}
}
