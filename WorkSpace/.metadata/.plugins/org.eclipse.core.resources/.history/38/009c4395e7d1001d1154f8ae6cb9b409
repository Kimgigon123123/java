
public class Ex04_Oper {public static void main(String[] args) {
	int value1=5,value2=3;
	System.out.println("value1 + value2 = " + (value1 + value2));
	System.out.println("value1 * value2 = " + (value1 * value2));
	System.out.println("value1 / value2 = " + (value1 / value2));
	System.out.println("value1 % value2 = " + (value1 % value2));
}

}
