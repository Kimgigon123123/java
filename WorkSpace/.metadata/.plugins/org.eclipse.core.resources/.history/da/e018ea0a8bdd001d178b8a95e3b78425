package pack01_base;

public class Ex06_MethodMain {
	public static void main(String[] args) {
		Ex06_Method em= new Ex06_Method();
		em.method1(); //void
		em.method2("입니다"); //void
		em.isMethod3(); //boolean
		em.isMethod4(10); //boolean
		//메소드에 리턴타입이 주어지면 해당하는 타입의 변수라고 봐도됨.
		// 메소드에 리턴 타입이 없으면 아무 타입도 아님 (실행후 없어지는 코드블럭)
		
		System.out.println(em.method1());
		System.out.println(em.isMethod3());
		
		boolean isMethod = em.isMethod3();
		
		//조건식 핵심: true or false 일것
		if(em.isMethod3()) { //메소드 결과가 true 일때 또는 false 일때에 따라서
			// if와 else 블럭 실행여부가 바뀜
			
		}
	}

}
