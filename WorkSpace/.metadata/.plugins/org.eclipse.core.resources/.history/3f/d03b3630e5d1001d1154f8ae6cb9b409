
public class Test02 { public static void main(String[] args) {
	int x = 1;
	int y = 1;
	int result1= ++x +10; //  연산(12)후 출력(12)
	int result2= y++ + 10; // 출력(11)후 연산(12)
	System.out.println(result1);
	System.out.println(result2);
	
	System.out.println(result2++);
	System.out.println(result2++);
	
}

}
